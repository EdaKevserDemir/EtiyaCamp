package intro;

public class Main {

	public static void main(String[] args) {
		int asallar = 0;

		for (int i = 2; i <= 1000; i++) {
			int check = 0;
			for (int j = 2; j < i; j++) {
				if (i % j == 0) {
					check = 1;
					break;
				}
			}

				if (check == 0) {
					System.out.println(i);
					asallar++;
				}

			}

		}

	}


